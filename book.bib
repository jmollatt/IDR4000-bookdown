	pmid = {25794868},
	keywords = {Adult, Athletic Injuries, eccentric strengthening, Exercise, Exercise Therapy, hamstring injuries, Humans, Incidence, injury prevention, Leg Injuries, Male, Muscle, Skeletal, nordic curl, nordic hamstring exercise, Risk, Risk Factors, soccer, Soccer, Soft Tissue Injuries, Surveys and Questionnaires, Thigh, Young Adult}
