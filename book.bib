	number = {6},
	journaltitle = {The American Journal of Sports Medicine},
	shortjournal = {Am J Sports Med},
	author = {van der Horst, Nick and Smits, Dirk-Wouter and Petersen, Jesper and Goedhart, Edwin A. and Backx, Frank J. G.},
